{
  "defaultSeverity": "error",
  "extends": [
    "tslint:latest",
    "tslint-config-prettier"
  ],
  "jsRules": {},
  "rulesDirectory": [
    "tslint-plugin-prettier"
  ],
  "rules": {
    "prettier": true,
    "no-console": false,
    "object-literal-shorthand": false,
    "no-submodule-imports": false,
    "no-string-literal": false
  }
}
